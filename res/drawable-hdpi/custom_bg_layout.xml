<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    
    <!-- focused -->
    <item android:state_focused="true" android:drawable="@color/white"></item>
    <!-- focused and pressed-->
    <item android:state_focused="false" android:state_pressed="true" android:drawable="@color/white"></item>
    <!-- pressed -->
    <item android:state_pressed="true" android:drawable="@color/white"></item>
    <!-- default -->
    <item android:drawable="@color/white" /> 
 
</selector>
